{
  "manifest_version": 2,
  "name": "CENO Intercept",
  "description": "Facilitates interaction with the CENO Client, your gateway to the CENO network.",
  "default_locale": "en",
  "version": "1.0.0",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "CENO Intercept",
    "default_icon": "icon.png",
    "default_popup": "__MSG_browserActionPage__"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "proxy",
    "https://*/*",
    "http://*/*"
  ] 
}
